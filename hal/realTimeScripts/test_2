#!/usr/bin/perl -w

print "called as $0\n";
if($0 !~ /test_2$/) {
  print "This is not test_2\n";
}
#chomp ($n=`ps ax`);
#print "$n instances of test\n";
($sec, $min, $hr) = gmtime;
printf "#STARTTIME %02d:%02d:%02d\n", $hr, $min, $sec;
#system("shmValue -a3 -n20 \"RM_DEWAR_TEMPS_V16_F[2,4]\"> tmp&");
print "Done\n";
